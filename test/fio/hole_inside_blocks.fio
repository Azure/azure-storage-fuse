[global]
name=blobfuse_file_integrity
bs=1M
filename=hole_inside_blocks_fio.data
verify=crc32c
verify_fatal=1
group_reporting

[hole]
size=10M
# Create 3M Hole after each write call.
# This will create the holes inside the blocks(assuming block size of 8M)
rw=write:3M
ioengine=sync
fallocate=none
unlink=1
