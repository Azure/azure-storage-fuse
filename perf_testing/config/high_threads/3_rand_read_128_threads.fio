[global]
ioengine=libaio
direct=1
fadvise_hint=0
verify=0
rw=read
iodepth=64
invalidate=1
ramp_time=10s
runtime=60s
time_based=1
nrfiles=1
thread=1
openfiles=1
group_reporting
allrandrepeat=1
filename_format=$jobname.$jobnum.$filenum
bs=1M
size=1G

[rand_read_128_thread]
stonewall
numjobs=128
rw=randread